{
    "name": "aspace_backend",
    "version": "1.0.0",
    "private": false,
    "description": "aspace, Inc. - Backend (node.js/Express.js stack), available at https://api.trya.space.",
    "dependencies": {
        "mkdirp": "^0.5.1",
        "nan": "^2.6.2",
        "node-cmake": "^2.3.2",
        "node-pre-gyp": "^0.11.0",
        "osrm-text-instructions": "^0.13.2",
        "promisify-any": "^2.0.1",
        "rimraf": "^2.5.4"
    },
    "browserify": {
        "transform": [
            "babelify",
            "brfs"
        ]
    },
    "scripts": {
        "lint": "node ./node_modules/eslint/bin/eslint.js -c ./.eslintrc features/step_definitions/ features/support/",
        "test": "npm run lint && node ./node_modules/cucumber/bin/cucumber.js features/ -p verify && node ./node_modules/cucumber/bin/cucumber.js features/ -p mld",
        "clean": "rm -rf test/cache",
        "docs": "./scripts/build_api_docs.sh",
        "install": "node-pre-gyp install --fallback-to-build=false || ./scripts/node_install.sh",
        "nodejs-tests": "make -C test/data && ./lib/binding/osrm-datastore test/data/ch/monaco.osrm && node test/nodejs/index.js | faucet",
        "start": "node app.js",
        "dev": "PORT=3001 node app.js",
        "prod": "PORT=3000 node app.js",
        "deploy": "pm2 start app.js --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@gitlab.com/aspace/router-backend.git"
    },
    "author": "aspace, Inc.",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://gitlab.com/aspace/backend/issues"
    },
    "homepage": "https://gitlab.com/aspace/backend#README",
    "engines": {
        "node": ">=4.0.0"
    },
    "devDependencies": {
        "@google/maps": "^0.5.5",
        "@mapbox/mapbox-sdk": "^0.4.1",
        "@slack/client": "^4.3.1",
        "@turf/turf": "^5.1.6",
        "ansi-escape-sequences": "^4.0.0",
        "aws-sdk": "^2.282.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "bcryptjs": "^2.4.3",
        "chalk": "^1.1.3",
        "command-line-args": "^5.0.2",
        "command-line-usage": "^5.0.4",
        "connect-timeout": "^1.9.0",
        "cors": "^2.8.4",
        "csv-stringify": "^3.0.0",
        "cucumber": "^1.2.1",
        "d3-queue": "^2.0.3",
        "docbox": "^1.0.11",
        "documentation": "^4.0.0-rc.1",
        "ejs": "^2.6.1",
        "eslint": "^2.4.0",
        "express": "^4.16.3",
        "express-cluster": "0.0.5",
        "express-errors-to-slack": "^1.0.3",
        "express-toobusy": "0.0.3",
        "faucet": "^0.0.1",
        "geojson": "^0.5.0",
        "graphql-request": "^1.7.0",
        "helmet": "^3.13.0",
        "jsonpath": "^1.0.0",
        "mathjs": "^5.1.1",
        "module-alias": "^2.1.0",
        "moment": "^2.22.2",
        "multer": "^1.3.1",
        "multer-s3": "^2.7.0",
        "mysql": "github:mysqljs/mysql",
        "node-timeout": "0.0.4",
        "polyline": "^0.2.0",
        "request": "^2.87.0",
        "request-promise": "^4.2.2",
        "serve-favicon": "^2.5.0",
        "sharp": "^0.20.5",
        "tape": "^4.7.0",
        "turf": "^3.0.14",
        "twilio": "^3.17.5",
        "unique-string": "^1.0.0",
        "xml": "^1.0.1",
        "xmlbuilder": "^4.2.1"
    },
    "bundleDependencies": [
        "mkdirp",
        "nan",
        "node-cmake",
        "node-pre-gyp",
        "rimraf"
    ],
    "main": "lib/index.js",
    "binary": {
        "module_name": "node_osrm",
        "module_path": "./lib/binding/",
        "host": "https://mapbox-node-binary.s3.amazonaws.com",
        "remote_path": "./{name}/v{version}/{configuration}/",
        "package_name": "{node_abi}-{platform}-{arch}.tar.gz"
    },
    "_moduleAliases": {
        "@root": ".",
        "@auth-bcrypt": "misc/auth/bcrypt",
        "@auth-basic": "misc/auth/basic",
        "@auth-user": "misc/auth/user",
        "@errors": "misc/errors/errorHandler",
        "@error-codes": "misc/errors/errorCodes",
        "@misc": "misc",
        "@config": "config",
        "@sql": "database/sqlActions",
        "@twilio": "misc/twilio",
        "@timestamp": "misc/timestamp",
        "@parking-calc": "misc/parkingCalc",
        "@bcrypt": "misc/auth/bcrypt",
        "@route-optimization": "misc/routeOptimization",
        "@body-structure": "config/bodyStructs",
        "@osrm": "app"
    },
    "_moduleDirectories": [
        "node_modules"
    ]
}
